        @html.block("js")
   </head>
    
    <body>
        <!-- menu-->
        <button type="button" class="btn btn-default navbar-btn">Sign in</button>
        <!-- end menu-->
        
    <div class="menu-top-div">
     <!--  <img class="header" src="/resources/img/kth-main.png" />-->
    <div class="weather">@html.raw(model.weather)</div>
</div>
    <div class="nav-menubar" id="divMenuBar">
        <ul>
           
        <li><a href="/">Home</a></li>
        <li><a  href="/music/">Folk Music</a></li>
        <li><a href="/gallery/">Gallery</a></li>
        <li><a href="/media/">News/Articles</a></li>
        <li><a href="/message/">My Message</a></li>                       
        <li><a href="/classified/">Classified</a></li>
        <li><a href="/trainstatus/">Live Train Status</a></li>
        <li><a href="/account/">Fellow KATHUAITES</a></li>
         @if (model.user==undefined){
        <li><a href="/login/">Log in</a></li>
        }
        else
        {
       
        <li><a href="/profile/">Profile</a>|<a href="/logout/">Log Out</a></li>
        }    
        </ul>
        
    </div>
     @if (model.user!=undefined){
     if(model.user.profile_image_url !=undefined)
     {
     <img src='@model.user.profile_image_url' alt='@model.user.displayName'/><p>Hello @model.user.displayName,</p>
     }
     else{
      <i>Hello @model.user.displayName,</i> 
     }
        
    }
    <div class="main">
            @html.block("main")
    </div>
    

    
    
    <footer>footer</footer>
    </body> 
    
</html>

